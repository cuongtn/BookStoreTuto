#{param name=listBooks/}

<div class="tutorial" style="padding: 10px">
<table border="1" cellspacing="3">
	<tr>
       <th>Name</th>
       <th>Categorie</th>
       <th>Content</th>
   	</tr>
	<%listBooks.each(){book->%>
	
   <tr>
       <td>${book.name}</td>
       <td>${book.categorie.toString()}</td>
       <td>${book.content}</td>
   </tr>
   <%}%>
</table>
   

   
</div>